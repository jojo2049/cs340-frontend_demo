<!DOCTYPE html>
<html lang="en">
    <head>
        <link rel="stylesheet" href="/static/style.css">
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>{{title}}</title>

        <script src="/static/common.js" defer></script>
        {{#each scripts}}
        <script src="/static/{{this}}" defer></script>
        {{/each}}
    </head>

    <body>
        <p><i>Must connect using OSU vpn!</i></p>
        <div>
            <a href="/home">Home</a>
            <a href="/users">Users</a>
            <a href="/recipes">Recipes</a>
            <a href="/food_items">FoodItems</a>
            <a href="/genres">Genres</a>
            <a href="/ingredients_table">IngredientsTable</a>
            <a href="/genres_table">GenresTable</a>
        </div>
        
        {{{body}}}

        <div class="container">
            <h3 style="margin: 0px 0px 0px 10px">{{title}} Data</h3>
            <div id="container_results_message"></div>
            <div id="container_results_table">
                <table>
                    {{#with results_table}}
                    <tr>
                        {{#each headers}}
                        <th>{{this}}</th>
                        {{/each}}
                    </tr>
                    {{#each rows}}
                    <tr>
                        {{#each this}}
                        <td>{{this}}</td>
                        {{/each}}
                    </tr>
                    {{/each}}
                    {{/with}}
                </table>
            </div>
        </div>
    </body>
</html>
